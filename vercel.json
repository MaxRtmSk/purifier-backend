{
    "version": 2,
    "env": {
        "POSTGRES_HOST": "@VERCEL_POSTGRES_HOST",
        "POSTGRES_PORT": "@VERCEL_POSTGRES_PORT",
        "POSTGRES_DB": "@VERCEL_POSTGRES_DB",
        "POSTGRES_USER": "@VERCEL_POSTGRES_USER",
        "POSTGRES_PASSWORD": "@VERCEL_POSTGRES_PASSWORD",
        "JWT_SECRET": "@VERCEL_JWT_SECRET",
        "JWT_EXPIRATION_TIME": "1000000"
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}
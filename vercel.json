{
    "version": 2,
    "env": {
        "POSTGRES_HOST": "@POSTGRES_HOST",
        "POSTGRES_PORT": "@POSTGRES_PORT",
        "POSTGRES_DB": "@POSTGRES_DB",
        "POSTGRES_USER": "@POSTGRES_USER",
        "POSTGRES_PASSWORD": "@POSTGRES_PASSWORD",
        "JWT_SECRET": "@JWT_SECRET",
        "JWT_EXPIRATION_TIME": "1000000"
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}